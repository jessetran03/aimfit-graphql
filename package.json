{
  "name": "aimfit-graphql",
  "version": "1.0.0",
  "main": "./dist/index",
  "types": "./dist/index",
  "license": "MIT",
  "engines": {
    "node": "16.7.0"
  },
  "scripts": {
    "migrate": "postgrator --config postgrator-config.js",
    "seed": "psql -U jessetran -d aimfit-postgres -f ./seeds/seed.aimfit_tables.sql",
    "dev": "nodemon src/index.ts",
    "start": "node dist",
    "build": "tsc",
    "predeploy": "yarn audit && yarn build",
    "deploy": "git push heroku main"
  },
  "dependencies": {
    "apollo-server-express": "^3.5.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "pg": "^8.7.1",
    "postgrator-cli": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
